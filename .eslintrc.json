{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 2],
        "quotes": ["error", "single"],
        "eqeqeq": "warn",
        "no-console": "off",
        "no-inline-comments": "off",
        "semi": ["error", "always"]
    }
}